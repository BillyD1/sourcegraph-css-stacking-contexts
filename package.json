{
  "name": "css-stacking-contexts",
  "displayName": "CSS Stacking Contexts",
  "description": "Highlights stacking contexts in CSS and ineffective z-index declarations.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Linters",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:scss"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "prettier": "@sourcegraph/prettierrc",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0",
    "@sourcegraph/prettierrc": "^3.0.3",
    "@sourcegraph/eslint-config": "^0.20.11",
    "@sourcegraph/tsconfig": "^4.0.1",
    "@types/lodash": "^4.14.162",
    "@types/dedent": "^0.7.0"
  },
  "dependencies": {
    "postcss": "^8.1.1",
    "postcss-scss": "^3.0.2",
    "lodash": "^4.17.20",
    "dedent": "^0.7.0"
  }
}
