{
  "name": "css-stacking-context-highlighter",
  "displayName": "CSS Stacking Context Highlighter",
  "description": "Highlight properties in CSS that introduce new stacking contexts",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:less"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "prettier": "@sourcegraph/prettierrc",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0",
    "@sourcegraph/prettierrc": "^3.0.3",
    "@sourcegraph/eslint-config": "^0.20.11",
    "@sourcegraph/tsconfig": "^4.0.1"
  },
  "dependencies": {
    "scss-tokenizer": "^0.4.2",
    "rxjs": "^6.6.3"
  }
}
